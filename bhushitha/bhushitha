package Training;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	Scanner sc =new Scanner(System.in);	
		int passenger = 0;
		int n= sc.nextInt();
		int m= sc.nextInt();
		int a[][]= new int[n][m];
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<m;j++)
			{
				a[i][j] = sc.nextInt();
			}
		}
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<m;j++)
			{
				if(a[i][j] == 1)
					passenger++;
			}
		}
		System.out.println(passenger);
		
		

	}

}